{
    "code-runner.runInTerminal": true,
    "code-runner.saveFileBeforeRun": true,
    "code-runner.executorMap": {
      "c": "cd $dir && gcc *.c  -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    },
    "files.associations": {
      "windows.h": "c",
      "type_traits": "c",
      "stdlib.h": "c",
      "ieeefp.h": "c",
      "stdio.h": "c",
      "stddef.h": "c",
      "stoplight.h": "c",
      "statemap.h": "c",
      "*.a": "c"
    },
  }